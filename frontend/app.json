{
  "expo": {
    "name": "Hacklahoma2026",
    "slug": "hacklahoma2026",
    "scheme": "hacklahoma2026",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "bundleIdentifier": "com.hacklahoma2026",
      "supportsTablet": true,
      "appleTeamId": "2H65T54FRU",
      "infoPlist": {
        "NSFamilyControlsUsageDescription": "Trail needs Screen Time access to help you stay focused and nudge you when you open distracting apps."
      }
    },
    "android": {
      "package": "com.hacklahoma2026"
    },
    "web": {},
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ],
      [
        "react-native-device-activity",
        {
          "appleTeamId": "2H65T54FRU",
          "appGroup": "group.com.hacklahoma2026"
        }
      ]
    ]
  }
}
